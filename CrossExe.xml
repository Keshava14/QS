      <class name="com.vtiger.testscripts.CreateNewProduct"/>
      <class name="com.vtiger.testscripts.CreateNewContact"/>
      <class name="com.vtiger.testscripts.CreateNewLeads"/>
    </classes>
  </test> <!-- Test -->
